<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:service ref="userService" interface="com.esther.code.api.IUserService"  group="${dubbo.esther.group}" version="${dubbo.esther.version}" />
    <dubbo:service ref="dictService" interface="com.esther.code.api.IDictService"  group="${dubbo.esther.group}" version="${dubbo.esther.version}" />
    <dubbo:service ref="ehcacheService" interface="com.esther.code.api.IEhcacheService"  group="${dubbo.esther.group}" version="${dubbo.esther.version}" />

</beans>