<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:vivo="http://www.vivo.com.cn/schema/vivo-service"
	   xmlns:vivoredis="http://www.vivo.com.cn/schema/vivo-service-redis"
	   xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.vivo.com.cn/schema/vivo-service-redis
        http://www.vivo.com.cn/schema/vivo-service-redis/vivo-service-redis.xsd
		http://www.vivo.com.cn/schema/vivo-service
    	http://www.vivo.com.cn/schema/vivo-service/vivo-service.xsd">


	<vivoredis:redis-cluster name="redisCluster" id="jedisCluster"/>

	<bean id="jedisClusterTemplate" class="com.vivo.framework.redis.cluster.JedisClusterTemplateFactory">
		<property name="jedisCluster" ref="jedisCluster"/>
	</bean>
</beans>
