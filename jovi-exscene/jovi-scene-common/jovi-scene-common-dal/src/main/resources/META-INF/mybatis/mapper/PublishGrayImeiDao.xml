<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vivo.jovi.scene.common.dal.dao.IPublishGrayImeiDao">
    <resultMap id="BaseResultMap" type="com.vivo.jovi.scene.common.dal.entity.PublishGrayImeiEntity">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="strategy_id" jdbcType="VARCHAR" property="strategyId"/>
        <result column="imei" jdbcType="VARCHAR" property="imei"/>
    </resultMap>

    <sql id="Base_Column_List">
        id, strategy_id, imei
    </sql>

    <insert id="insert" parameterType="com.vivo.jovi.scene.common.dal.entity.PublishGrayImeiEntity">
        insert into t_publish_gray_imei(strategy_id, imei)
        values (#{strategyId, jdbcType=VARCHAR}, #{imei, jdbcType=VARCHAR})
    </insert>

    <select id="getPublishGrayImei" resultMap="BaseResultMap">
        select
        *
        from t_publish_gray_imei
        where strategy_id = #{strategyId, jdbcType=VARCHAR}
        and imei = #{imei, jdbcType=VARCHAR}
    </select>
</mapper>